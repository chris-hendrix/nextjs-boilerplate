{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "npx prisma migrate dev --skip-seed",
    "up": "docker-compose -f docker-compose.yml up -d && docker-compose logs -f",
    "down": "docker-compose -f docker-compose.yml down",
    "devtest": "NODE_ENV=test && next dev",
    "jest": "NODE_ENV=test jest --config=test/jest/jest.config.ts --runInBand --forceExit --verbose",
    "cypress:open": "cypress open --config-file test/cypress/cypress.config.ts",
    "cypress": "cypress run --config-file test/cypress/cypress.config.ts",
    "seed": "ts-node ./prisma/seed.ts",
    "unseed": "ts-node ./prisma/unseed.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.4.2",
    "@reduxjs/toolkit": "^1.9.7",
    "@supabase/supabase-js": "^2.38.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/validator": "^13.11.3",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "autoprefixer": "^10.4.16",
    "bcrypt": "^5.1.1",
    "cypress": "^13.3.1",
    "daisyui": "^3.9.2",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^13.5.4",
    "jest": "^29.7.0",
    "next": "^13.5.4",
    "next-auth": "^4.23.2",
    "node-mocks-http": "^1.13.0",
    "postcss": "^8.4.31",
    "prisma": "^5.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-redux": "^8.1.3",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "unique-names-generator": "^4.7.1",
    "validator": "^13.11.0"
  }
}
