{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "npx prisma migrate dev --skip-seed",
    "up": "docker-compose -f docker-compose.yml up -d && docker-compose logs -f",
    "down": "docker-compose -f docker-compose.yml down",
    "devup": "next dev && npm run up",
    "jest": "NODE_ENV=test jest --config=jest/jest.config.ts --runInBand --forceExit"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^4.15.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "18.2.4",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "autoprefixer": "10.4.14",
    "bcrypt": "^5.1.0",
    "daisyui": "^3.0.3",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.4.4",
    "jest": "^29.5.0",
    "next": "^13.4.4",
    "next-auth": "^4.22.1",
    "postcss": "^8.4.24",
    "prisma": "^4.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "react-redux": "^8.0.7",
    "tailwindcss": "3.3.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "validator": "^13.9.0"
  }
}
